---
interface Props {
    educador: string,
}
const {educador} = Astro.props as Props;
---
<section class="py-12">
    <header>
        <h2 class="font-bold leading-tighter tracking-tighter font-heading text-heading text-3xl md:text-4xl">{educador}</h2>
        <div class="text-xl text-muted mb-6 dark:text-slate-300 py-5">
            <slot name="resumen" >
        </div>
    </header>
    <slot/>
</section>